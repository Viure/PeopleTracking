<?xml version="1.0"?>
<configuration>
  <appSettings>

    <add key="StoreID" value="Samsung 1"/>    
    
    <!-- START : Settings for Exracting Images from the VIDEO -->

    <!-- Load video timer in mins  -->
    <add key="LoadVideoTimer" value="1"/>

    <!--Source video path.  -->
    <add key="InputVideoPath" value="D:\Projects\Image Processing\Human tracking\Database\Video"/>

    <!--Extracted images would be saved at below location.  -->
    <add key="SaveImageLocation" value="D:\Projects\Image Processing\Human tracking\Database\Images"/>
    
    <!--TimeDifferenceToExtractImageFromVideo in seconds  -->
    <add key="TimeDifferenceToExtractImageFromVideo" value="20"/>
    
    <!-- Extracted images from Video will be resized based on below value. the value is in DOUBLE  -->
    <add key="OutFrameResizeScalefactor" value="3.0"/>

    <!-- END : Settings for Exracting Images from the VIDEO -->

    <!-- =====================================================================================================================================================================-->
    
    <!--START : Settings for Face detection -->

    <!--Auto face size is enable or not ? if yes then it will be calculated based on the given percentages and image width and height. -->
    <add key="IsAutoFaceSize" value="false"/>
    <!--face size will be calculated based on the given percentage of image width and height. -->
    <add key="FaceRectanglePercentage" value="2"/>

    <!--is the size of the smallest face to search for. you can select the default for this by setting the scale to ,say, 25x25, as in our code -->
    <add key="OutFaceSize" value="50"/>

    <!--Setting this higher makes the detector run faster (by running fewer passes), but if it's too high, you may jump too quickly between scales and miss faces.
    This parameter may have a value of 1.1 , 1.2 , 1.3 or 1.4.-->
    <add key="ScaleFactor" value="1.1"/>
    
    <!--‘The minimum-neighbors threshold’ which sets the cutoff level for discarding or keeping rectangle groups as “face” or not, based on how many raw detections are in the group.-->
    <add key="MinNeighbors" value="4"/>

    <!--END : Settings for Face detection -->

    <!-- =====================================================================================================================================================================-->

    <!--START : Settings for Gender detection -->

   
    <!--END : Settings for Gender detection -->
    
    <!-- =====================================================================================================================================================================-->
   
    <!--START : Feed XML creation -->

    <!--Extracted Feed xml's would be saved at below location.  -->
    <add key="OutXMLLocation" value="D:\Projects\Image Processing\Human tracking\Database\Out_XML"/>
    
    <!-- END : Feed XML creation -->
    
  </appSettings>
  
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
